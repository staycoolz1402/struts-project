<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
  
<hibernate-mapping package="com.ams.mufins.model">
	<class
		name="PersonalContact"
		table="personal_contact"
		batch-size="100"
	>
	
		<meta attribute="sync-DAO">true</meta>
	
		<id
			name="Id"
			type="long"
			column="personal_id"
		>
		</id>
		
		<property 
			name="Name" 
			column="name" 
			type="string" 
		/>
		
		<property 
			name="Address" 
			column="address" 
			type="string" 
		/>
		
		<property 
			name="Rt" 
			column="rt" 
			type="string" 
		/>
		
		<property 
			name="Rw" 
			column="rw" 
			type="string" 
		/>
		
		<property 
			name="PostalCodeId" 
			column="postal_code_id" 
			type="long" 
		/>
		
		<property 
			name="Province" 
			column="province" 
			type="string" 
		/>
		
		<property 
			name="City" 
			column="city" 
			type="string" 
		/>
		
		<property 
			name="Subdistrict" 
			column="subdistrict" 
			type="string" 
		/>
		
		<property 
			name="Village" 
			column="village" 
			type="string" 
		/>
		
		<property 
			name="PostalCode" 
			column="postal_code" 
			type="string" 
		/>
		
		<property 
			name="PhoneNumber" 
			column="phone_number" 
			type="string" 
		/>
		
		<property 
			name="RelationshipId" 
			column="relationship_id" 
			type="long" 
			not-null="true"
		/>
		
		<property 
			name="CreateBy" 
			column="create_by" 
			type="string" 
		/>
		
		<property 
			name="CreateOn" 
			column="create_on" 
			type="timestamp" 
		/>
		
		<property 
			name="ChangeBy" 
			column="change_by" 
			type="string" 
		/>
		
		<property 
			name="ChangeOn" 
			column="change_on" 
			type="timestamp" 
		/>
		
	</class>	
</hibernate-mapping>