#dialog-konfirmasi-red {
    width: 100%;
    height: 100%;
    background-color: #E64444;
    position: fixed;
    z-index: 1000;
    right: 0;
    bottom: 0;
}
#dialog-konfirmasi-green {
    width: 100%;
    height: 100%;
    background-color: #72B930;
    position: fixed;
    z-index: 1000;
    right: 0;
    bottom: 0;
}

#dialog-bg-red {
    background-color: #FBFBFB;
    position: fixed;
	width: 300px;
	height: 100px;
	top: 50%;
	left: 50%;
	margin-top: -50px; /* Negative half of height. */
	margin-left: -150px; /* Negative half of width. */
    -webkit-border-radius: 7px; /* Saf3-4, iOS 1-3.2, Android <1.6 */
    -moz-border-radius: 7px; /* FF1-3.6 */
    border-radius: 7px; /* Opera 10.5, IE9, Saf5, Chrome, FF4, iOS 4, Android 2.1+ */
}

#dialog-bg-green {
    background-color: #FBFBFB;
    position: fixed;
	width: 300px;
	height: 100px;
	top: 50%;
	left: 50%;
	margin-top: -50px; /* Negative half of height. */
	margin-left: -150px; /* Negative half of width. */
    -webkit-border-radius: 7px; /* Saf3-4, iOS 1-3.2, Android <1.6 */
    -moz-border-radius: 7px; /* FF1-3.6 */
    border-radius: 7px; /* Opera 10.5, IE9, Saf5, Chrome, FF4, iOS 4, Android 2.1+ */
}

#dialog-title-red {
    width: 300px;
    height: 30px;
    padding-top: 12px;
    background-color: #FFFFFF;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#FFFFFF), to(#E9E9E9)); /* Saf4+, Chrome */
    background-image: -webkit-linear-gradient(top, #FFFFFF, #E9E9E9); /* Chrome 10+, Saf5.1+, iOS 5+ */
    background-image: -moz-linear-gradient(top, #FFFFFF, #E9E9E9); /* FF3.6 */
    background-image: -ms-linear-gradient(top, #FFFFFF, #E9E9E9); /* IE10 */
    background-image: -o-linear-gradient(top, #FFFFFF, #E9E9E9); /* Opera 11.10+ */
    background-image: linear-gradient(top, #FFFFFF, #E9E9E9);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#FFFFFF', EndColorStr='#E9E9E9'); /* IE6-IE9 */
    border-top-left-radius: 7px;
    border-top-right-radius: 7px;
    border-bottom: 1px solid #FFFFFF;
    color: #434343;
    text-shadow: 1px 1px 1px #ffffff;
    filter: dropshadow(color=#ffffff, offx=1, offy=1);
    text-align: center;
    font-weight: bold;
    font-size: 14px;
    cursor: default;
}

#dialog-title-green {
    width: 300px;
    height: 30px;
    padding-top: 12px;
    background-color: #FFFFFF;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#FFFFFF), to(#E9E9E9)); /* Saf4+, Chrome */
    background-image: -webkit-linear-gradient(top, #FFFFFF, #E9E9E9); /* Chrome 10+, Saf5.1+, iOS 5+ */
    background-image: -moz-linear-gradient(top, #FFFFFF, #E9E9E9); /* FF3.6 */
    background-image: -ms-linear-gradient(top, #FFFFFF, #E9E9E9); /* IE10 */
    background-image: -o-linear-gradient(top, #FFFFFF, #E9E9E9); /* Opera 11.10+ */
    background-image: linear-gradient(top, #FFFFFF, #E9E9E9);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#FFFFFF', EndColorStr='#E9E9E9'); /* IE6-IE9 */
    border-top-left-radius: 7px;
    border-top-right-radius: 7px;
    border-bottom: 1px solid #FFFFFF;
    color: #434343;
    text-shadow: 1px 1px 1px #ffffff;
    filter: dropshadow(color=#ffffff, offx=1, offy=1);
    text-align: center;
    font-weight: bold;
    font-size: 14px;
    cursor: default;
}

#dialog-buttons-red {
    width: 280px;
    margin-left: auto;
    margin-right: auto;
    margin-top: 20px;
    text-align: center;
}

.red.button.confirmation,
.red.button.confirmation:visited {
	font-size: 11px;
	text-decoration: none;
    background-color: #E64444;
    color: #fff;
    border: 1px solid #B63440;
    padding-left: 15px;
    padding-right: 15px;
    margin-left: 15px;
    background-image: url(../images/confirm_no.png);
    background-repeat: repeat-x;
    position: absolute;
}

.red.button.confirmation:hover {
    background-color: #FF4B5A;
    color: #000;
    border: 1px solid #B63440;
    padding-left: 15px;
    padding-right: 15px;
    position: absolute;
    cursor:pointer;
}

#dialog-buttons-green {
    width: 280px;
    margin-left: auto;
    margin-right: auto;
    margin-top: 20px;
    text-align: center;
}

.green.button.confirmation,
.green.button.confirmation:visited {
	font-size: 11px;
	text-decoration: none;
    background-color: #72B930;
    color: #fff;
    border: 1px solid #5D8C22;
    padding-left: 5px;
    padding-right: 5px;
    margin-left: -60px;
    background-image: url(../images/confirm_yes.png);
    background-repeat: repeat-x;
    position: absolute;
}

.green.button.confirmation:hover {
    background-color: #7DCA34;
    color: #000;
    border: 1px solid #5D8C22;
    padding-left: 5px;
    padding-right: 5px;
    position: absolute;
    cursor:pointer;
}
